{
  "name": "code-exercise",
  "version": "1.0.1",
  "description": "Simple code exercises.",
  "main": "index.js",
  "scripts": {
    "start": "serve static",
    "build": "NODE_PATH=source browserify source/index.js -o static/index.js -t [ babelify --presets [ es2015 react stage-2 ] ]",
    "lint": "eslint source",
    "test": "NODE_PATH=source babel-node source/test/index.js",
    "watch": "watch 'clear && npm run -s lint && npm run -s test && npm run -s build' source",
    "sync": "browser-sync start --server static --files 'static'",
    "update": "updtr && depcheck --ignores='babelify'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jshomes/code-exercise.git"
  },
  "author": "Eric Elliott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jshomes/code-exercise/issues"
  },
  "homepage": "https://github.com/jshomes/code-exercise#readme",
  "dependencies": {
    "react": "15.1.0",
    "react-dom": "15.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "babelify": "7.3.0",
    "browser-sync": "2.12.10",
    "browserify": "13.0.1",
    "depcheck": "0.6.3",
    "enzyme": "2.9.1",
    "eslint": "3.7.1",
    "eslint-plugin-react": "6.4.1",
    "react-addons-test-utils": "15.1.0",
    "serve": "1.4.0",
    "tape": "4.5.1",
    "updtr": "0.1.15",
    "watch": "0.18.0"
  }
}
